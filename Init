local ClickSound = Instance.new("Sound")
ClickSound.Volume = 1
ClickSound.Parent = DarkHub
ClickSound.SoundId = "rbxassetid://1238528678"

-- Fetch data from webhost

CreditsValue.Text = darkCore.import("Credits")
PatchNotesValue.Text = darkCore.import("PatchNotes")

BackButton.MouseButton1Click:Connect(function()
	ThemesList:TweenPosition(UDim2.new(1, 0, 0.116666667, 0), "Out","Sine", .2)
end)

ThemeButton.MouseButton1Click:Connect(function()
	ClickSound:Play()
	ThemesList:TweenPosition(UDim2.new(1, -170, 0.116666667, 0), "In","Sine", .2)
end)

StartButton.MouseButton1Click:Connect(function()
	ClickSound:Play()
	Background:TweenPosition(UDim2.new(0, -400, 0.5, -150), "In","Sine", .2)
	for i,v in next, darkCore.supportedGames do
		if tostring(game.PlaceId) == tostring(v) then
			loadstring(game:HttpGet("https://raw.githubusercontent.com/RandomAdamYT/DarkHub/master/" .. tostring(i)))()
		end
	end
end)

-- Slide in UI when everything's done
Background:TweenPosition(UDim2.new(0, 0, 0.5, -150), "Out","Sine", .5)
