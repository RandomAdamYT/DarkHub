MainWindow:Toggle("Rainbow Hands", function(state)
Client.Visuals.Rainbow = state
end)

local RunService = game:GetService("RunService")
RunService.Heartbeat:Connect(function()
    if Client.Visuals.Rainbow == true then
    wait(.45)
for i, v in pairs(game:GetService("Workspace").Arms:GetChildren()) do
    v:IsA("Part")
    v.Color = Color3.fromHSV(tick()%5/5,1,1)
    v.Material = "ForceField"
    v.Transparency = 0.5
end
else
    wait(.45)
for i, v in pairs(game:GetService("Workspace").Arms:GetChildren()) do
    v:IsA("Part")
    v.Color = Color3.fromHSV(243, 187, 19)
    v.Material = "SmoothPlastic"
    v.Transparency = 0

    
end
end
end)  
end

MainWindow:Toggle("Rainbow Gun", function(state)
game:GetService("Workspace").ChildAdded:Connect(function(model)
    local RunService = game:GetService("RunService")
RunService.Heartbeat:Connect(function()
    wait(.45)
    if model:FindFirstChild("RightHand") then
        for i, v in pairs(model.Body:GetDescendants()) do
            if v.ClassName == "MeshPart" and v.Transparency ~= 1 then
                v.Color = Color3.fromHSV(tick()%5/5,1,1)
                v.Material = "ForceField"
                v.Transparency = 0.5
            end
        end

    end
end)
end)
end)




MainWindow:Slider("Speed", 2, 10, function(num)
    for i,v in pairs(getgc(true)) do
        if type(v) == 'table' and rawget(v,'Control') and type(v.Control) == 'function' then
            if table.find(getconstants(v.Control),'Stand') then
                print(getconstant(v.Control,tonumber(table.find(getconstants(v.Control),'Stand'))+1))
                setconstant(v.Control,tonumber(table.find(getconstants(v.Control),'Stand'))+1,num)
            end
        end
    end
end)


